import React from 'react'
import { useLocation } from 'react-router-dom'
import GoodList from './GoodList'


const Good = () => {
  const path = useLocation()
  const pathname = path.pathname
  const render = (path) => {
    switch (path) {
      case "/good/":
       return  <GoodList />
      case "/good/upload":
       return  <GoodList />
      default:
       return  <GoodList />
    }
  }
  return (
    <div>
    {render(pathname)
    }
    </div>
  )
}

export default Good
